<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="500dp" android:height="500dp">
        <shape android:layout_width="match_parent">
            <gradient
                android:angle="89"
                android:endColor="@color/aqua2"
                android:gradientRadius="20dp"
                android:startColor="@color/aqua1" />

            <stroke
                android:width="1dp"
                android:color="@color/aqua2" />
            <corners android:radius="360dp" />
            <padding
                android:left="120dp"
                android:right="120dp" />
        </shape>
    </item>

    <item android:top="150dp" android:bottom="320dp" android:width="255dp" android:height="30dp">
        <shape android:shape="rectangle" android:tint="@color/ui_dark_element">
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:top="300dp" android:bottom="170dp" android:width="255dp" android:height="30dp">
        <shape android:shape="rectangle" android:tint="@color/ui_dark_element">
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:top="225dp" android:bottom="245dp" android:width="255dp" android:height="30dp">
        <shape android:shape="rectangle" android:tint="@color/ui_dark_element">
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>